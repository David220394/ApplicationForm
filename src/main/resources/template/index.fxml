<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.applicationform.Controller.IndexController">
   <children>
      <HBox alignment="CENTER_LEFT" layoutX="23.0" layoutY="14.0" maxHeight="-Infinity" prefHeight="50.0" style="-fx-background-color: #222fa5;">
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
         <children>
            <ImageView fitHeight="43.0" fitWidth="135.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="2.5" />
               </HBox.margin>
               <image>
                  <Image url="@../static/image/Total_logo.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" styleClass="grid" stylesheets="@../static/index.css" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="toRegister" mnemonicParsing="false" onAction="#toRegister" stylesheets="@../static/button.css" text="Register">
               <HBox.margin>
                  <Insets bottom="50.0" left="180.0" right="50.0" top="120.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </Button>
            <Button mnemonicParsing="false" onAction="#login" stylesheets="@../static/button.css" text="Login">
               <HBox.margin>
                  <Insets right="100.0" top="120.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </Button>
         </children></HBox>
   </children>
</VBox>
